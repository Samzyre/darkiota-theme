		"list.highlightForeground": "#e08010",
		"list.invalidItemForeground": "#c02010",
		"button.foreground": "#e08010",
		"button.background": "#404040",
		"button.hoverBackground": "#505050",
		"badge.background": "#202020",
		"badge.foreground": "#e08010",
		"scrollbar.shadow": "#00000080",
		"scrollbarSlider.background": "#f0f0f020",
