		"tab.unfocusedHoverBackground": "#40404080",
		"tab.border": "#00000080",
		"tab.activeBorder": "#e08010",
		"tab.unfocusedActiveBorder": "#808080",
		"tab.hoverBorder": "#00000080",
		"tab.unfocusedHoverBorder": "#00000080",
