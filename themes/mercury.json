{
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"colors": {
		"foreground": "#a0a0a0",
		"errorForeground": "#c02010",
		"descriptionForeground": "#808080",
		"focusBorder": "#00000080",
		"selection.background": "#e0801080",
		"textSeparator.foreground": "#c0c0c0",
		"textLink.foreground": "#6090c0",
		"textLink.activeForeground": "#9060c0",
		"textPreformat.foreground": "#808080",
		"textBlockQuote.background": "#1d1e1f80",
		"textBlockQuote.border": "#10101080",
		"textCodeBlock.background": "#2d2e2f80",
		"widget.shadow": "#00000080",
		"input.background": "#303030",
		"input.foreground": "#c0c0c0",
		"input.border": "#00000080",
		"inputOption.activeBorder": "#00000080",
		"input.placeholderForeground": "#808080",
		"inputValidation.infoBackground": "#101010",
		"inputValidation.infoBorder": "#1080c080",
		"inputValidation.warningBackground": "#101010",
		"inputValidation.warningBorder": "#e0801080",
		"inputValidation.errorBackground": "#101010",
		"inputValidation.errorBorder": "#c0201080",
		"dropdown.background": "#303030",
